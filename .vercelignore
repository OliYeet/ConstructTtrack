# Vercel Ignore File
# Files and directories to exclude from Vercel deployment

# Development and validation scripts
scripts/validate-*.js
scripts/openapi-*.js
scripts/test-api-*.js

# API validation documentation
docs/api/validation/

# Development configuration
.env.development
.env.staging

# Test files
**/__tests__/
**/*.test.ts
**/*.test.js
**/*.spec.ts
**/*.spec.js

# Development tools
.husky/
.lintstagedrc.json

# Notion scripts (development only)
notion_scripts/

# IDE files
.vscode/
.idea/
*.swp
*.swo

# OS files
.DS_Store
Thumbs.db

# Logs
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Runtime data
pids
*.pid
*.seed
*.pid.lock

# Coverage directory used by tools like istanbul
coverage/
*.lcov

# Temporary folders
tmp/
temp/
